%section.new-tag
  .admin-title-header
    %h2 New Tag
    = link_to "Back to Tags", admin_tags_path
  = form_for @tag, url: admin_tags_path, method: :post do |f|
    = render partial: 'error_handler', locals: { model: @tag }
    %p
      = f.label :label
      = f.text_field :label
    %p= f.submit
